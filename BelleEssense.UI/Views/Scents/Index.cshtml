@model IEnumerable<BelleEssense.DATA.Scent>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ScentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ScentDesc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ScentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ScentDesc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ScentId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ScentId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ScentId })
        </td>
    </tr>
}

</table>
